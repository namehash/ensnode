{
  "$schema": "https://unpkg.com/@changesets/config@3.1.1/schema.json",
  "changelog": ["@changesets/changelog-github", { "repo": "namehash/ensnode" }],
  "commit": true,
  "access": "public",
  "baseBranch": "main",
  "fixed": [
    [
      "ensindexer",
      "ensadmin",
      "ensrainbow",
      "@ensnode/datasources",
      "@ensnode/ensrainbow-sdk",
      "@ensnode/ponder-metadata",
      "@ensnode/ensnode-schema",
      "@ensnode/ponder-subgraph",
      "@ensnode/ensnode-sdk",
      "@ensnode/shared-configs"
    ]
  ],
  "updateInternalDependencies": "patch",
  "prettier": true,
  "privatePackages": {
    "version": true,
    "tag": true
  },
  "ignore": ["@docs/ensnode", "@docs/ensrainbow"]
}
