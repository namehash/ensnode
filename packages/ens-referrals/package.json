{
  "name": "@namehash/ens-referrals",
  "version": "1.5.1",
  "type": "module",
  "description": "Utilities for ENS Referrals.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namehash/ensnode.git",
    "directory": "packages/ens-referrals"
  },
  "homepage": "https://github.com/namehash/ensnode/tree/main/packages/ens-referrals",
  "keywords": [
    "ENS",
    "Referrals"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./v1": "./src/v1/index.ts",
    "./internal": "./src/internal.ts",
    "./v1/internal": "./src/v1/internal.ts"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/index.cjs"
        }
      },
      "./v1": {
        "import": {
          "types": "./dist/v1/index.d.ts",
          "default": "./dist/v1/index.js"
        },
        "require": {
          "types": "./dist/v1/index.d.cts",
          "default": "./dist/v1/index.cjs"
        }
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepublish": "tsup",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "viem": "catalog:"
  },
  "dependencies": {
    "@ensnode/ensnode-sdk": "workspace:*",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@ensnode/shared-configs": "workspace:*",
    "@types/node": "catalog:",
    "tsup": "^8.3.6",
    "typescript": "catalog:",
    "viem": "catalog:",
    "vitest": "catalog:"
  }
}
