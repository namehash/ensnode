{
  "name": "@namehash/namehash-ui",
  "version": "1.3.1",
  "type": "module",
  "description": "Opinionated UI components for use in apps published by NameHash Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namehash/ensnode.git",
    "directory": "packages/namehash-ui"
  },
  "homepage": "https://github.com/namehash/ensnode/tree/main/packages/namehash-ui",
  "keywords": [
    "NameHash",
    "React",
    "UI"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./src/index.ts"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    },
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "styles": "./dist/index.css"
  },
  "scripts": {
    "prepublish": "tsup",
    "dev": "tsup --watch",
    "lint": "biome check --write .",
    "lint:ci": "biome ci",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@ensnode/ensnode-react": "workspace:*",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@ensnode/shared-configs": "workspace:*",
    "@testing-library/react": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "react": "19.2.1",
    "tsup": "^8.3.6",
    "typescript": "catalog:",
    "@tailwindcss/postcss": "^4.1.18",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18"
  },
  "dependencies": {}
}
