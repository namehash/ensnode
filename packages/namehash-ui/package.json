{
  "name": "@namehash/namehash-ui",
  "version": "1.5.1",
  "type": "module",
  "description": "Opinionated UI components for use in apps published by NameHash Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namehash/ensnode.git",
    "directory": "packages/namehash-ui"
  },
  "homepage": "https://github.com/namehash/ensnode/tree/main/packages/namehash-ui",
  "keywords": [
    "NameHash",
    "React",
    "UI"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./styles.css": "./dist/index.css"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./styles.css": {
        "import": "./dist/index.css",
        "default": "./dist/index.css"
      }
    },
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "styles": "./dist/index.css"
  },
  "scripts": {
    "postinstall": "pnpm build:css",
    "prepublish": "pnpm build:css && tsup",
    "build:css": "postcss src/styles.css -o dist/index.css",
    "dev": "tsup --watch",
    "lint": "biome check --write .",
    "lint:ci": "biome ci",
    "typecheck": "tsgo --noEmit",
    "test": "vitest"
  },
  "peerDependencies": {
    "@ensnode/ensnode-react": "workspace:*",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.3"
  },
  "devDependencies": {
    "@ensnode/shared-configs": "workspace:*",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/react": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "react": "19.2.1",
    "tailwindcss": "catalog:",
    "tsup": "^8.3.6",
    "typescript": "catalog:",
    "viem": "catalog:",
    "vitest": "catalog:"
  },
  "dependencies": {
    "@ensnode/datasources": "workspace:*",
    "@ensnode/ensnode-sdk": "workspace:*",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "boring-avatars": "^2.0.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "catalog:",
    "lucide-react": "catalog:",
    "tailwind-merge": "catalog:",
    "tailwindcss-animate": "catalog:",
    "tw-animate-css": "^1.4.0"
  }
}
