    "yargs": "^17.7.2",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1"
  },
  "devDependencies": {
    "@ensnode/shared-configs": "workspace:*",
    "@types/node": "catalog:",
    "@types/yargs": "^17.0.32",
    "@vitest/coverage-v8": "catalog:",
    "tsx": "^4.19.3",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
