{
  "name": "ensnode-monorepo",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "lint": "biome check --write .",
    "lint:ci": "biome ci",
    "changeset": "changeset",
    "packages:build": "pnpm -r --filter=\"@ensnode/*\" --filter=\"!@ensnode/utils\" --filter=\"!@ensnode/shared-configs\" build",
    "packages:dev": "pnpm -r --filter=\"@ensnode/*\" --filter=\"!@ensnode/utils\" --filter=\"!@ensnode/shared-configs\" dev"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.27.12",
    "tsup": "catalog:",
    "typescript": "catalog:"
  },
  "engines": {
    "node": ">=18.20.5"
  },
  "pnpm": {
    "overrides": {
      "ponder>vite": "5.4.14"
    }
  }
}
